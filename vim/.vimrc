nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
syntax on
colorscheme peachpuff
set paste
set smartindent
set autoindent
set cindent
set number
set mouse+=a
set nowrap
"set term=xterm
"set ttymouse=xterm2
"set nocompatible
command WQ wq
command Wq wq
command W w
command Q q
